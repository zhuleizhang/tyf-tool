{
  "name": "tyf-tool",
  "version": "2.2.0",
  "description": "üçë ÁöÑÂ∑•ÂÖ∑ÁÆ±",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "build:main": "webpack --config webpack.main.config.js",
    "build:app": "webpack --config webpack.config.js",
    "build": "rm -rf dist && npm run build:main && npm run build:app",
    "watch": "webpack --watch",
    "dev": "npm run build && electron .",
    "package:win": "rm -rf dist && cross-env NODE_ENV=production npm run build:main && cross-env NODE_ENV=production npm run build:app && electron-builder --win --x64 --publish never",
    "package:mac": "rm -rf dist && cross-env NODE_ENV=production npm run build:main && cross-env NODE_ENV=production npm run build:app && electron-builder --mac --publish never"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^24.13.3",
    "electron-dialog": "^1.0.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "asarUnpack": [
      "dist/service/**/*"
    ],
    "appId": "com.tyf.tool",
    "productName": "TYF Tool",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "src/assets/logo.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/logo.icns",
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "src/assets/logo.ico",
      "uninstallerIcon": "src/assets/logo.ico",
      "installerHeaderIcon": "src/assets/logo.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "files": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/sharp": "^0.31.1",
    "antd": "^5.26.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "heic2any": "^0.0.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  }
}